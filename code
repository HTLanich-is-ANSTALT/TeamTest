print("Dieser Code ist jetzt super")
# Hundeklasse
class Dog:
    # Konstruktor zum Erstellen der Objekte
    def __init__(self, name, rasse):
        # Attribute / Instanzvariablen / Membervariablen
        self.name = name
        self.rasse = rasse
    # Methoden
    def greet(self, bellzahl):
        print(f"My name is {self.name}, i am a dog haha. I am a {self.rasse}.")
        for i in range(bellzahl):
            print("Wuff")
        
# Instanzen erzeugen
mein_hund = Dog("Wuffi", "Sch√§ferhund")
nachbar_hund = Dog("Schnuffi", "Pudel")

# Methoden aufrufen
mein_hund.greet(10)
nachbar_hund.greet(3)


